function ChangeOngletAccueil(onglet, contenu) {
				document.getElementById('resume').style.display = 'none';
				document.getElementById('host').style.display = 'none';
				document.getElementById('datastore').style.display = 'none';
				document.getElementById('listcours').style.display = 'none';
				document.getElementById('user').style.display = 'none';
				document.getElementById(contenu).style.display = 'block';

				document.getElementById('tab_resume').className = '';
				document.getElementById('tab_host').className = '';
				document.getElementById('tab_datastore').className = '';
				document.getElementById('tab_vm').className = '';
				document.getElementById('tab_user').className = '';

			}
function ChangeOngletHost(onglet, contenu) 
{   
    document.getElementById('resume').style.display = 'none';
    document.getElementById('listcours').style.display = 'none';
    document.getElementById(contenu).style.display = 'block';       
 
    document.getElementById('tab_resume').className = '';
    document.getElementById('tab_vm').className = '';
          
}  