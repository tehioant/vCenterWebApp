body {
    position: relative;
    overflow-x: hidden;
}
body,
html { height: 100%;}
.nav .open > a, 
.nav .open > a:hover, 
.nav .open > a:focus {background-color: transparent;}

/*-------------------------------*/
/*           Wrappers            */
/*-------------------------------*/
header{
	padding-left : 150px;
}


#wrapper {
    padding-left: 0;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

#wrapper.toggled {
    padding-left: 220px;
}
/*-------------------------------*/
/*            Overlay            */
/*-------------------------------*/

.overlay {
    position: fixed;
    display: none;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(250,250,250,.8);
    z-index: 1;
}


#Titre{
	text-align : center;
	font-familly : sans-serif;
}

#conteneur{
	display : flex;
    justify-content: space-between;
}

table {
	border: thin solid #6495ed;
	width: 50%;
	height: 150%;
}

th {
	font-family: sans-serif;
	border: thin solid #6495ed;
	width: 50%;
	padding: 5px;
}

caption {
	font-family: sans-serif;
	border: thin solid #6495ed;
	text-align: center;
}

body {
    position: relative;
    overflow-x: hidden;
}




