package fr.eseo.cc3.model.vm;

import static org.junit.Assert.assertEquals;

import org.junit.Test;

public class TestHardware {

	@Test
	public void testUpgradePolicy() {
		Hardware hardware = new Hardware();
		hardware.setUpgradePolicy("testUpgradePolicy");
		assertEquals(hardware.getUpgradePolicy(),"testUpgradePolicy");
	}
	@Test
	public void testUpgradeStatus() {
		Hardware hardware = new Hardware();
		hardware.setUpgradeStatus("testUpgradeStatus");
		assertEquals(hardware.getUpgradeStatus(),"testUpgradeStatus");
	}
	
	@Test
	public void testVersion() {
		Hardware hardware = new Hardware();
		hardware.setVersion("testVersion");
		assertEquals(hardware.getVersion(),"testVersion");
	}
}
